{
  "name": "mez/queuejitsu-scheduler-cli",
  "type": "library",
  "description": "QueueJitsu Scheduler CLI",
  "license": "MIT",
  "authors": [
    {
      "name": "Martin Meredith",
      "email": "martin@sourceguru.net"
    }
  ],
  "require": {
    "php": "^7.2",
    "ext-pcntl": "*",
    "laminas/laminas-config-aggregator": "^1.0",
    "mez/queuejitsu-cli": "^0.2",
    "mez/queuejitsu-scheduler": "^0.2.2 || ^0.3",
    "symfony/console": "^5.1"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.6",
    "friendsofphp/php-cs-fixer": "^2.16",
    "laminas/laminas-development-mode": "^3.2",
    "laminas/laminas-servicemanager": "^3.4",
    "nikic/php-parser": "^4.6",
    "php-parallel-lint/php-parallel-lint": "^1.2",
    "phpro/grumphp-shim": "^0.19.1 || ^0.21.0",
    "phpstan/phpstan": "^0.12.32",
    "predis/predis": "^1.1",
    "roave/security-advisories": "dev-master",
    "sensiolabs/security-checker": "^6.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "stickee/*": "source",
      "mez/*": "source",
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "QueueJitsu\\Scheduler\\Cli\\": [
        "src"
      ]
    }
  }
}
