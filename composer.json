{
    "name": "mez/queuejitsu-scheduler-cli",
    "description": "QueueJitsu Scheduler CLI",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Meredith",
            "email": "martin@sourceguru.net"
        }
    ],
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "preferred-install": {
            "stickee/*": "source",
            "mez/*": "source",
            "*": "dist"
        }
    },
    "require": {
        "mez/queuejitsu": "dev-master",
        "mez/queuejitsu-cli": "dev-master",
        "mez/queuejitsu-scheduler": "dev-master",
        "symfony/console": "^3.3",
        "zendframework/zend-config-aggregator": "^1.0",
        "zfcampus/zf-development-mode": "^3.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.4",
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "nikic/php-parser": "^3.1",
        "phpmd/phpmd": "^2.6",
        "phpro/grumphp": "^0.11.6",
        "phpstan/phpstan": "^0.8.3",
        "povils/phpmnd": "^1.1",
        "predis/predis": "^1.1",
        "sebastian/phpcpd": "^3.0",
        "sensiolabs/security-checker": "^4.1",
        "squizlabs/php_codesniffer": "^3.0",
        "vlucas/phpdotenv": "^2.4",
        "zendframework/zend-servicemanager": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "QueueJitsu\\Scheduler\\Cli\\": [
                "src"
            ]
        }
    }
}
